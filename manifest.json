{
  "manifest_version": 2,
  "name": "Tor Bridges Scanner",
  "version": "1.0",
  "icons": {
    "48": "images/tor.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "browser_action": {
    "default_title": "Tor Bridges Scanner",
    "default_popup": "index.html"
  }
}
