{
  "manifest_version": 2,
  "name": "BridgeMeTheTor",
  "version": "0.1",
  "description":"Take Tor bridges - here",
  "browser_action": {
    "default_title": "BridgeMeTheTor",
    "default_icon": "images/logo_16.png",
    "default_popup": "index.html"
  },
  "icons": {
    "16": "images/logo_16.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ]
    }
  ],
  "permissions": [
    "tabs",
    "http://*/*",
    "https://*/*",
    "webNavigation",
    "webRequest",
    "webRequestBlocking"
  ],
  "browser_action": {
    "default_title": "BridgeMeTheTor"
  }
}
