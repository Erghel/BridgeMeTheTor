{
  "manifest_version": 2,
  "name": "I love Tor",
  "version": "1.0",
  "icons": {
    "16": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": [
        "script.js"
      ],
      "matches": [
        "http://*/*",
        "https://*/*"
      ]
    }
  ],
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage",
    "tabs"
  ],
  "browser_action": {
    "default_title": "I love Tor",
    "default_popup": "index.html"
  }
}
